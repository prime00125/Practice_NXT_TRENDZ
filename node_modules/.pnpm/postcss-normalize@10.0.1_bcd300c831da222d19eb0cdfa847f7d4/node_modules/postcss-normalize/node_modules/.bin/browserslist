#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxttrendz/node_modules/.pnpm/browserslist@4.23.0/node_modules/browserslist/cli.js/node_modules:/home/nxtwave/nxttrendz/node_modules/.pnpm/browserslist@4.23.0/node_modules/browserslist/node_modules:/home/nxtwave/nxttrendz/node_modules/.pnpm/browserslist@4.23.0/node_modules:/home/nxtwave/nxttrendz/node_modules/.pnpm/node_modules:/home/nxtwave/nxttrendz/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxttrendz/node_modules/.pnpm/postcss-normalize@10.0.1_bcd300c831da222d19eb0cdfa847f7d4/node_modules/postcss-normalize/node_modules:/home/nxtwave/nxttrendz/node_modules/.pnpm/postcss-normalize@10.0.1_bcd300c831da222d19eb0cdfa847f7d4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.23.0/node_modules/browserslist/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../browserslist@4.23.0/node_modules/browserslist/cli.js" "$@"
fi
