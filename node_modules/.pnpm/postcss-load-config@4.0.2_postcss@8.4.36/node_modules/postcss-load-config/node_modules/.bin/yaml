#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxttrendz/node_modules/.pnpm/yaml@2.4.1/node_modules/yaml/bin.mjs/node_modules:/home/nxtwave/nxttrendz/node_modules/.pnpm/yaml@2.4.1/node_modules/yaml/node_modules:/home/nxtwave/nxttrendz/node_modules/.pnpm/yaml@2.4.1/node_modules:/home/nxtwave/nxttrendz/node_modules/.pnpm/node_modules:/home/nxtwave/nxttrendz/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxttrendz/node_modules/.pnpm/postcss-load-config@4.0.2_postcss@8.4.36/node_modules/postcss-load-config/node_modules:/home/nxtwave/nxttrendz/node_modules/.pnpm/postcss-load-config@4.0.2_postcss@8.4.36/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../yaml@2.4.1/node_modules/yaml/bin.mjs" "$@"
else 
  exec node  "$basedir/../../../../../yaml@2.4.1/node_modules/yaml/bin.mjs" "$@"
fi
